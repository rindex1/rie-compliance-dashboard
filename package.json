{
  "name": "rie-compliance-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "prisma generate --schema prisma/schema.prisma && prisma db push --schema prisma/schema.prisma --skip-generate && next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint",
    "vercel-build": "prisma generate --schema prisma/schema.postgres.prisma && next build",
    "postinstall": "prisma generate --schema prisma/schema.postgres.prisma",
    "seed": "node prisma/seed.js",
    "seed:risks": "node prisma/seed-risks.js",
    "db:generate:pg": "prisma generate --schema prisma/schema.postgres.prisma",
    "db:migrate:pg": "prisma migrate deploy --schema prisma/schema.postgres.prisma"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.16.3",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@stripe/stripe-js": "^8.2.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "docx": "^9.5.1",
    "framer-motion": "^12.23.24",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.544.0",
    "next": "15.5.4",
    "postcss": "^8.5.6",
    "prisma": "^6.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "react-hot-toast": "^2.6.0",
    "recharts": "^3.2.1",
    "stripe": "^19.2.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "pg": "^8.16.3",
    "typescript": "^5"
  }
}
